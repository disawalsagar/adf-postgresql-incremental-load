{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF55879"
		},
		"AzureDataLakeStorageGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageGen2'"
		},
		"AzurePostgreSql1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzurePostgreSql1'"
		},
		"AzureTableStorageLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureTableStorageLS'"
		},
		"azureBlobStorageSourceContainer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azureBlobStorageSourceContainer'"
		},
		"population_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'population'"
		},
		"AzureDataLakeStorageGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://bronze12456.dfs.core.windows.net/"
		},
		"AzurePostgreSql1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dbpostgresql5698.postgres.database.azure.com"
		},
		"AzurePostgreSql1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "test12"
		},
		"AzurePostgreSql1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "disawalsagar"
		},
		"ecdc_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF55879_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageGen2_accountKey": {
						"value": "[parameters('AzureDataLakeStorageGen2_accountKey')]"
					},
					"AzurePostgreSql1_password": {
						"value": "[parameters('AzurePostgreSql1_password')]"
					},
					"AzureTableStorageLS_connectionString": {
						"value": "[parameters('AzureTableStorageLS_connectionString')]"
					},
					"azureBlobStorageSourceContainer_connectionString": {
						"value": "[parameters('azureBlobStorageSourceContainer_connectionString')]"
					},
					"population_connectionString": {
						"value": "[parameters('population_connectionString')]"
					},
					"AzureDataLakeStorageGen2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageGen2_properties_typeProperties_url')]"
					},
					"AzurePostgreSql1_properties_typeProperties_server": {
						"value": "[parameters('AzurePostgreSql1_properties_typeProperties_server')]"
					},
					"AzurePostgreSql1_properties_typeProperties_database": {
						"value": "[parameters('AzurePostgreSql1_properties_typeProperties_database')]"
					},
					"AzurePostgreSql1_properties_typeProperties_username": {
						"value": "[parameters('AzurePostgreSql1_properties_typeProperties_username')]"
					},
					"ecdc_ls_properties_typeProperties_url": {
						"value": "[parameters('ecdc_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}